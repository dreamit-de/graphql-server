  "version": "3.2.1",
  "description": "A GraphQL server written in NodeJS/Typescript.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit --pretty",
