    "@sgohlke/ts-base-dependencies": "1.0.3",
    "@types/content-type": "1.1.5",
    "@types/express": "4.17.17",
    "cross-fetch": "3.1.5",
    "eslint-plugin-deprecation": "1.3.3",
    "express": "4.18.2",
    "jest-html-reporters": "3.1.3"
